{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@sections/*": [
        "./src/components/sections/*"
      ],
      "@configs/*": [
        "./src/configs/*"
      ],
      "@ts/*": [
        "./src/ts/*"
      ],
      "@scss/*": [
        "./src/scss/*"
      ]
    }
  }
}